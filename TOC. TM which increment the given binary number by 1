#include <iostream>
#include <string>
using namespace std;

bool incrementTM(string &tape) {
    int head = tape.size() - 1;
    while (head >= 0) {
        if (tape[head] == '0') {
            tape[head] = '1';
            return true;}
        tape[head] = '0';
        head--;
    }
    tape.insert(tape.begin(), '1');
    return true;
}

int main() {
    string tape;
    cout << "Enter binary string: ";
    cin >> tape;
	incrementTM(tape);
    cout << "Incremented string: " << tape;
    return 0;
}
